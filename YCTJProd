                    List<int> condition2PP = new List<int>();

                    if (condition0.Count - 1 != conditionPP.Count)
                    {
                        continue;
                    }

                    if (!string.IsNullOrEmpty(item.condition))
                    {
                        //1表示>=  2表示> 3表示小于等于 4表示小于  5表示=  6表示!=
                        condition2 = item.condition.Replace("and", "$").Split('$').ToList();
                        string condition2str = item.condition;

                        #region Oddset
                        if (condition2str.Contains("hostWeight500>="))
                        {
                            double provalue = double.Parse(condition2.Where(s => s.Contains("hostWeight500>=")).FirstOrDefault().Replace("hostWeight500>=", ""));
                            if (weightFinal.hostWeight500 >= provalue)
                            {
                                condition2PP.Add(1);
                            }
                        }
                        if (condition2str.Contains("hostWeight1000>="))
                        {
                            double provalue = double.Parse(condition2.Where(s => s.Contains("hostWeight1000>=")).FirstOrDefault().Replace("hostWeight1000>=", ""));
                            if (weightFinal.hostWeight1000 >= provalue)
                            {
                                condition2PP.Add(1);
                            }
                        }
                        if (condition2str.Contains("hostWeight2000>="))
