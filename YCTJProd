                    string[] condition2 = item.condition2.Split('|');

                    //第一位 H 表示主队输赢   G表示客队输赢
                    //第二位 10 表示取最近多少场次
                    //第三位 80 表示最近场次赢的百分比
                    //第四位 Y 表示赢球结果  N表示输球结果
                    //H|10|80|Y
                    int CC = Convert.ToInt32(condition2[1]);
                    double BFB = Convert.ToDouble(condition2[2]);
                    if (condition2[0] == "H")
                    {
                        List<WeightFinalALLProd> matchWeightFinalFilter = matchWeightFinalALL.Where(glambda.Compile()).OrderByDescending(a => a.id).Take(CC).ToList();

                        if (condition2[3] == "Y")
                        {
                            if (matchWeightFinalFilter.Count(a => a.hostResult == "y") * 1.0 / CC >= BFB * 1.0 / 100)
                            {
                                weightFinal.YCResult = weightFinal.YCResult + item.id + ":" + "Y";

                                if (!string.IsNullOrEmpty(hostResult))
                                {
                                    if (hostResult == "y")
                                    {
                                        item.HostYCWin = item.HostYCWin + 1;
                                    }
                                    else
                                    {
                                        item.HostYCPing = item.HostYCPing + 1;
                                    }
                                    item.HostTotal = item.HostTotal + 1;
                                }
                            }
                        }

                        if (condition2[3] == "N")
                        {
                            if (matchWeightFinalFilter.Count(a => a.hostResult == "n") * 1.0 / CC >= BFB * 1.0 / 100)
                            {
                                weightFinal.YCResult = weightFinal.YCResult + item.id + ":" + "N";

                                if (!string.IsNullOrEmpty(hostResult))
                                {
                                    if (hostResult == "n")
                                    {
                                        item.HostYCWin = item.HostYCWin + 1;
                                    }
                                    else
                                    {
                                        item.HostYCPing = item.HostYCPing + 1;
                                    }
                                    item.HostTotal = item.HostTotal + 1;
                                }
                            }
                        }
                    }

