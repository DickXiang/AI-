                    List<int> condition2PP = new List<int>();

                    if (!string.IsNullOrEmpty(item.condition))
                    {
                        //1表示>=  2表示> 3表示小于等于 4表示小于  5表示=  6表示!=

                        conditionvalue = item.condition.Replace("and", "$").Split('$').ToList();
                        string conditions = item.condition;
                        #region Oddset
                        if (conditions.Contains("hostWeight500>="))
                        {
                            double provalue = double.Parse(conditionvalue.Where(s => s.Contains("hostWeight500>=")).FirstOrDefault().Replace("hostWeight500>=", ""));
                            if (weightFinal.hostWeight500 >= provalue)
