                    {
                        conditionPP.Add(1);
                    }
                    else if (string.IsNullOrEmpty(weightFinal.EurobetgameType))
                    {
                        if (condition0.Any(a => a.Trim().Equals($"[EurobetgameType] is null")))
                        {
                            conditionPP.Add(1);
                        }
                        if (condition0.Any(a => a.Trim().Equals($"[EurobetgameType] is not null")))
                        {
                            conditionPP.Add(1);
