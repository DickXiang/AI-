using ProxyCollect.Model.QTFTDB;
using ProxyCollect.Tool;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Linq.Expressions;
using System.Text;
using System.Threading.Tasks;

namespace ProxyCollect.Prod
{
    public static class YCTJProd
    {
        public static void YC(WeightFinalALLProd weightFinal, List<WeightFinalTJProd> tjList, List<WeightFinalALLProd> matchWeightFinalALL, string hostResult = "", string guestResult = "")
        {
            foreach (var item in tjList.Where(a => a.gameType == weightFinal.AMgameType && a.prodType == weightFinal.prodType).ToList())
            {
                if (!string.IsNullOrEmpty(item.condition2))
                {
                    Expression<Func<WeightFinalALLProd, bool>> glambda = x => 1 == 1;
                    if (!string.IsNullOrEmpty(item.condition0))
                    {
                        List<string> condition0 = item.condition0.Replace("and", "$").Split('$').ToList();
                        List<int> conditionPP = new List<int>();
                        string gameTypes = item.condition0;
                        #region 类型
                        if (gameTypes.Contains($"[gameType]='{weightFinal.gameType}'") && !string.IsNullOrEmpty(weightFinal.gameType))
                        {
                            glambda = LambdaCreate.createLambdaWeightFinalstrProd(glambda, "gameType", weightFinal.gameType, false, true);
                            conditionPP.Add(1);
                        }
                        else if (gameTypes.Contains($"[gameType]!='{weightFinal.gameType}'") && !string.IsNullOrEmpty(weightFinal.gameType))
                        {
                            glambda = LambdaCreate.createLambdaWeightFinalstrProd(glambda, "gameType", weightFinal.gameType, false, false);
                            conditionPP.Add(1);
                        }
                        else if (string.IsNullOrEmpty(weightFinal.gameType))
                        {
                            if (gameTypes.Contains($"[gameType] is null"))
                            {
                                glambda = LambdaCreate.createLambdaWeightFinalstrProd(glambda, "gameType", string.Empty, true, true);
                                conditionPP.Add(1);
                            }
                            if (gameTypes.Contains($"[gameType] is not null"))
                            {
                                glambda = LambdaCreate.createLambdaWeightFinalstrProd(glambda, "gameType", string.Empty, false, true);
                                conditionPP.Add(1);
                            }
