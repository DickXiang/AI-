                    if (condition2.Count == condition2PP.Count + 1)
                    {
                        if (!string.IsNullOrEmpty(item.HostResult))
                        {
                            weightFinal.YCResult = weightFinal.YCResult + item.id + ":" + item.HostResult;

                            if (!string.IsNullOrEmpty(hostResult))
                            {
                                if (item.HostResult.Contains(hostResult.ToUpper()))
                                {
                                    item.HostYCWin = item.HostYCWin + 1;
                                }
                                else
                                {
                                    item.HostYCPing = item.HostYCPing + 1;
                                }
                                item.HostTotal = item.HostTotal + 1;
                            }
                        }

                        if (!string.IsNullOrEmpty(item.GuestResult))
                        {
                            weightFinal.YGPResult = weightFinal.YGPResult + item.id + ":" + item.GuestResult;

                            if (!string.IsNullOrEmpty(guestResult))
                            {
                                if (item.GuestResult.Contains(guestResult.ToUpper()))
                                {
                                    item.GuestYCWin = item.GuestYCWin + 1;
                                }
                                else
                                {
                                    item.GuestYCPing = item.GuestYCPing + 1;
                                }
                                item.GuestTotal = item.GuestTotal + 1;
                            }
                        }
                    }
                }
            }
        }

